{
	"compilerOptions": {
		"strict": true,
		"module": "ES6",
		"moduleResolution": "node",
		"lib": ["es2015", "es2016", "es2017", "es2018", "es2019", "es2020"],
		"target": "ES5",
		"outDir": "./dist",
		"esModuleInterop": true,
		"allowJs": true,
		"baseUrl": "./src",
		"jsx": "react-jsx"
	},
	"exclude": ["node_modules"],
	"include": ["src/**/*"]
}
