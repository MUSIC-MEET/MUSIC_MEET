<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.musicmeet_intelij">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
            android:allowBackup="true"
            android:icon="@drawable/ic_launcher_background"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.MusicMeet_InteliJ"
            android:usesCleartextTraffic="true">
        <activity
                android:name=".FindMusicActivity"
                android:exported="true"/>
        <activity
                android:name=".MusicLiveChartActivity"
                android:exported="true"/>
        <activity
                android:name=".ex"
                android:exported="true"
                android:label="@string/title_activity_ex"
                android:theme="@style/Theme.MusicMeet_InteliJ.NoActionBar"/>
        <activity
                android:name=".Music_Main_Activity"
                android:exported="true"/>
        <activity
                android:name=".Members_Activity"
                android:exported="true"/>
        <activity
                android:name=".Login_Activity"
                android:exported="true"/>
        <activity
                android:name=".MainActivity"
                android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>