<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MusicMeet_InteliJ" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/status_bar_Color</item>
        <!--?attr/colorPrimaryVariant-->
        <!-- Customize your theme here. -->
        <item name="android:windowFullscreen">false</item>
        <!--풀스크린 만들기-->
        <item name="windowNoTitle">true</item>
        <!--앱바, 액션바 지우기-->
    </style>

    <style name="Theme.MusicMeet_InteliJ.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.MusicMeet_InteliJ.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>

    <style name="Theme.MusicMeet_InteliJ.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
</resources>